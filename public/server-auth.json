{
  "name": "帳號權限申請/異動",
  "tickets": [
    {
      "title": "Server adm權限申請-正式環境",
      "inputs": [
        {
          "type": "textarea",
          "label": "目的",
          "key": "purpose"
        },
        {
          "type": "text",
          "label": "申請人",
          "key": "applicant"
        },
        {
          "type": "text",
          "label": "申請人部門",
          "key": "department"
        },
        {
          "type": "text",
          "label": "申請環境",
          "key": "enviorment"
        },
        {
          "type": "textarea",
          "label": "申請 Server 清單(Host/IP/Remark)",
          "key": "server_list"
        },
        {
          "type": "textarea",
          "label": "申請期限",
          "key": "apply_dateline"
        },
        {
          "type": "description",
          "label": "需求說明",
          "key": "server_auth_description",
          "template": "[環境]\n${enviorment}\n${server_list}\n\n[需求說明]\n${purpose}\n申請人 : ${applicant}\n申請人部門 : ${department}\n申請環境 : ${enviorment} \n申請 Server 清單: \nHost\t IP\t Remark\n${server_list}\n\n申請期限 : ${apply_dateline}"
        }
      ]
    }
  ]
}