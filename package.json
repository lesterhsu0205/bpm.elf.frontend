{
  "name": "com.line.bank.bxi.bpm.elf.frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "pnpm run build:webcomponent && NODE_EXTRA_CA_CERTS=certs/lbtw.ca.starlbtwsys.20230719.20321231.crt next dev",
    "build:dev.local": "pnpm run webcomponent:dev.local && pnpm run next:dev.local && pnpm run server:dev.local",
    "build:dev": "pnpm run webcomponent:dev && pnpm run next:dev && pnpm run server:dev",
    "build:stg": "pnpm run webcomponent:stg && pnpm run next:stg && pnpm run server:stg",
    "build:uat": "pnpm run webcomponent:uat && pnpm run next:uat && pnpm run server:uat",
    "build:prod": "pnpm run webcomponent:prod && pnpm run next:prod && pnpm run server:prod",
    "start": "pnpm run build:webcomponent && NODE_EXTRA_CA_CERTS=dist/bpm-elf-frontend/certs/lbtw.ca.starlbtwsys.20230719.20321231.crt node --require ./dist/bpm-elf-frontend/patch-https.js dist/bpm-elf-frontend/server.js",
    "lint": "cross-env next lint",
    "build:webcomponent": "cross-env dotenv -e .env.development -- webpack --config webpack.webcomponent.config.js",
    "webcomponent:dev.local": "cross-env dotenv -e .env.dev.local -- webpack --config webpack.webcomponent.config.js",
    "webcomponent:dev": "cross-env dotenv -e .env.dev -- webpack --config webpack.webcomponent.config.js",
    "webcomponent:stg": "cross-env dotenv -e .env.stg -- webpack --config webpack.webcomponent.config.js",
    "webcomponent:uat": "cross-env dotenv -e .env.uat -- webpack --config webpack.webcomponent.config.js",
    "webcomponent:prod": "cross-env dotenv -e .env.prod -- webpack --config webpack.webcomponent.config.js",
    "next:dev.local": "cross-env dotenv -e .env.dev.local -- next build",
    "next:dev": "cross-env dotenv -e .env.dev -- next build",
    "next:stg": "cross-env dotenv -e .env.stg -- next build",
    "next:uat": "cross-env dotenv -e .env.uat -- next build",
    "next:prod": "cross-env dotenv -e .env.prod -- next build",
    "server:dev.local": "cross-env DOTENV_CONFIG_PATH=.env.dev.local node -r dotenv/config build.js",
    "server:dev": "cross-env DOTENV_CONFIG_PATH=.env.dev node -r dotenv/config build.js",
    "server:stg": "cross-env DOTENV_CONFIG_PATH=.env.stg node -r dotenv/config build.js",
    "server:uat": "cross-env DOTENV_CONFIG_PATH=.env.uat node -r dotenv/config build.js",
    "server:prod": "cross-env DOTENV_CONFIG_PATH=.env.prod node -r dotenv/config build.js"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.20",
    "@fontsource/noto-sans-tc": "^5.2.6",
    "@heroicons/react": "^2.2.0",
    "@material-tailwind/react": "^2.1.10",
    "autoprefixer": "10.4.16",
    "bootstrap": "^5.3.3",
    "clipboard": "^2.0.11",
    "json-edit-react": "~1.23.1",
    "lodash": "^4.17.21",
    "next": "14.1.4",
    "postcss": "8.4.32",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.6",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.57.0",
    "react-toastify": "^10.0.5",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@babel/runtime": "^7.27.6",
    "@stylistic/eslint-plugin": "^4.4.1",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.3",
    "postcss-loader": "^8.1.1",
    "react-to-webcomponent": "^2.0.1",
    "style-loader": "^4.0.0",
    "tar": "^7.4.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4"
  }
}
