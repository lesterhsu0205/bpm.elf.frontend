{
  "name": "com.line.bank.bxi.bpm.elf.frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "cross-env NODE_EXTRA_CA_CERTS=certs/ca.crt next dev",
    "build:dev": "dotenv -e .env.dev -- next build && node build.js",
    "build:stg": "dotenv -e .env.stg -- next build && node build.js",
    "build:uat": "dotenv -e .env.uat -- next build && node build.js",
    "build:prod": "dotenv -e .env.prod -- next build && node build.js",
    "start": "cross-env NODE_EXTRA_CA_CERTS=certs/ca.crt node --require ./dist/opt/sw/bpm.elf.frontend/{host}-01/nodejs/patch-https.js dist/opt/sw/bpm.elf.frontend/{host}-01/nodejs/server.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.20",
    "@heroicons/react": "^2.2.0",
    "@material-tailwind/react": "^2.1.10",
    "autoprefixer": "10.4.16",
    "bootstrap": "^5.3.3",
    "clipboard": "^2.0.11",
    "json-edit-react": "^1.16.3",
    "lodash": "^4.17.21",
    "next": "^14.2.15",
    "postcss": "8.4.32",
    "react": "18.2.0",
    "react-bootstrap": "^2.10.4",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.1",
    "react-toastify": "^10.0.5",
    "tailwindcss": "3.4.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "tar": "^7.4.3"
  }
}
